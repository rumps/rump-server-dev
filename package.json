{
  "name": "rump-server-dev",
  "version": "0.7.0",
  "author": "Juan Placencia",
  "description": "Rump piece for adding a development server to the flow",
  "license": "MIT",
  "homepage": "https://github.com/rumps/server-dev",
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/rumps/server-dev"
  },
  "scripts": {
    "preci": "npm run lint",
    "ci": "babel-node node_modules/.bin/isparta cover --report json _mocha",
    "postci": "codecov < coverage/coverage.json",
    "precompile": "[ -d lib ] && trash lib || true",
    "compile": "babel src --out-dir lib --source-maps=inline",
    "coverage": "babel-node node_modules/.bin/isparta cover --report html --report text _mocha -- -R min",
    "lint": "eslint src test",
    "pub": "npm run test && npm run compile && npm publish",
    "pretest": "npm run lint",
    "test": "mocha"
  },
  "dependencies": {
    "chalk": "^1.1.0",
    "connect-history-api-fallback": "^1.1.0",
    "browser-sync": "^2.8.2",
    "extend": "^3.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.19",
    "babel-eslint": "^4.0.5",
    "codecov.io": "^0.1.5",
    "eslint": "^1.0.0",
    "graceful-fs": "^4.1.2",
    "gulp": "^3.9.0",
    "isparta": "^3.0.3",
    "mocha": "^2.2.5",
    "mz": "^2.0.0",
    "node-fetch": "^1.3.2",
    "rump": "^0.7.6",
    "rump-static": "^0.7.0",
    "should": "^7.0.2",
    "should-sinon": "0.0.3",
    "sinon": "^1.15.4",
    "timeout-then": "^1.0.0",
    "trash": "^2.0.0"
  }
}
